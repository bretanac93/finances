              <li > <a href="{{ path('savingplan_index') }}">Planes de Ahorros</a> </li>
              <li > <a href="#">Capital Neto</a> </li>
            </ul>
        </li>
        {% if active == 'grupo cuentas' %}<li class="active open ">{% else %}<li>{% endif %}
          <a href="javascript:;"> <i class="fa fa-folder-open"></i> <span class="title">Grupo de Cuentas</span> <span class="arrow "></span> </a>
